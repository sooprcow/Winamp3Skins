<!-- Image Elements -->
<elements>
 <truetypefont id="custom.font.hooge553" file="xml/gui/fonts/HOOG0553.ttf" />

 <bitmap id="winframe.top.gradient" file="$gradient" w="1" h="5" gradient_x1="0" gradient_y1="0" gradient_x2="1" gradient_y2="1" points="0.0=255,255,255,255;1.0=255,255,255,0" />
 <bitmap id="winframe.bot.gradient" file="$gradient" w="1" h="25" gradient_x1="0" gradient_y1="0" gradient_x2="1" gradient_y2="1" points="0.0=255,255,255,0;1.0=255,255,255,255" />

 <bitmap id="winframe.titlebar.top.gradient" file="$gradient" w="1" h="10" gradient_x1="0" gradient_y1="0" gradient_x2="1" gradient_y2="1" points="0.0=55,90,225,255;1.0=55,90,225,0" gammagroup="Frame Titlebar Background" />
 <bitmap id="winframe.titlebar.bot.gradient" file="$gradient" w="1" h="4" gradient_x1="0" gradient_y1="0" gradient_x2="1" gradient_y2="1" points="0.0=55,90,225,0;1.0=55,90,225,255" gammagroup="Frame Titlebar Background" />

 <bitmap id="custom.grid.3" file="$polygon" points="0,0;1,0;1,3;0,3;0,0=0,0,0|0,0;01;3,1;3,0;0,0=0,0,0" w="3" h="3" gammagroup="Panel" />
 <bitmap id="panel.border.gradient" file="$gradient" w="1" h="50" gradient_x1="0" gradient_y1="0" gradient_x2="1" gradient_y2="1" points="0.0=18,28,70,255;1.0=18,28,70,0" gammagroup="Panel Dark" />

 <!-- Title Bar Icons -->
 <bitmap id="winframe.icon.minimize" file="xml/gui/images/window-minimize.png" gammagroup="Button Icons" />
 <bitmap id="winframe.icon.state" file="xml/gui/images/window-state.png" gammagroup="Button Icons" />
 <bitmap id="winframe.icon.close" file="xml/gui/images/window-close.png" gammagroup="Button Icons" />
 
 <!-- Slider Images -->
 <bitmap id="custom.slider.thumb" image="$solid" w="1" h="1" alpha="0" />
</elements>

