 <button id="shade" x="237" y="7" w="10" h="10" image="titlebar.shade" downimage="titlebar.shade.down" hoverimage="titlebar.shade.down" action="SWITCH" param="normal" tooltip="Switch To Normal Mode" />
 <button id="close" x="254" y="7" w="10" h="10" image="titlebar.close" downimage="titlebar.close.down" hoverimage="titlebar.close.down" action="close" tooltip="Close Winamp" />
</groupdef>


