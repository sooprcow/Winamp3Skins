<!-- Shademode Groups -->
<groupdef id="group.shade1.titlebar">
 <layer id="background" image="titlebar.background" x="0" y="0" w="0" h="0" relatw="1" relath="1" dblClickAction="SWITCH;shade2" />
 <layer id="logo" image="titlebar.logo" x="6" y="4" w="73" h="14" tooltip="Double Click To Launch DeskMod.com!!!"/>
 <button id="min" x="220" y="7" w="10" h="10" image="titlebar.min" downimage="titlebar.min.down" hoverimage="titlebar.min.down" action="Minimize" tooltip="Minimize Winamp" />
 <button id="shade" x="237" y="7" w="10" h="10" image="titlebar.shade" downimage="titlebar.shade.down" hoverimage="titlebar.shade.down" action="SWITCH" param="shade2" tooltip="Switch To Shademode 2" />
 <button id="close" x="254" y="7" w="10" h="10" image="titlebar.close" downimage="titlebar.close.down" hoverimage="titlebar.close.down" action="close" tooltip="Close Winamp" />
</groupdef>

<groupdef id="group.shade2.titlebar">
