<groupdef id="wasabi.standardframe.statusbar" background="frame.middle" sysregion="1" xuitag="Wasabi:StandardFrame:Status">
 <snappoint id="mainbot" x="0" y="0" />
 <snappoint id="mainright" x="0" y="0" />
 <group id="win.frame" x="0" y="0" w="0" h="0" relatw="1" relath="1"/>
 <LayoutStatus id="standardframe.layoutstatus" x="6" y="-19" w="-10" h="15" relaty="1" relatw="1" exclude="{system}" />
 <layer x="0" y="0" w="3" h="3" resize="topleft" />
 <layer x="3" y="0" w="-6" h="3" relatw="1" resize="top" />
 <layer x="-3" y="0" w="3" h="3" relatx="1" resize="topright" />
 <layer x="0" y="3" w="3" h="-6" relath="1" resize="left" />
 <layer x="-3" y="3" w="3" h="-6" relatx="1" relath="1" resize="right" />
 <layer x="0" y="-3" w="3" h="3" relaty="1" resize="bottomleft" />
 <layer x="3" y="-3" w="-6" h="3" relatw="1" relaty="1" resize="bottom" />
 <layer x="-6" y="-6" w="6" h="6" relatx="1" relaty="1" resize="bottomright" />
 <script id="standardframe.script" file="scripts/standardframe.maki" param="4,23,-10,-44,0,0,1,1" />
 
 <text x="4" y="5" w="0" h="19" relatw="1" valign="top" ticker="1" text=":componentname" showlen="1" font="player.font.normal" color="150,180,150" nograb="0" move="1" shadowcolor="150,180,150" shadowx="1" shadowy="1" fontsize="10" bold="1" align="left" />
 <text x="4" y="5" w="0" h="19" relatw="1" valign="top" ticker="1" text=":componentname" showlen="1" font="player.font.normal" color="150,180,150" nograb="0" move="1" shadowcolor="150,180,150" shadowx="-1" shadowy="-1" fontsize="10" bold="1" align="left" />
 <text x="4" y="5" w="0" h="19" relatw="1" valign="top" ticker="1" text=":componentname" showlen="1" font="player.font.normal" color="150,180,150" nograb="0" move="1" shadowcolor="150,180,150" shadowx="-1" shadowy="1" fontsize="10" bold="1" align="left" />
 <text x="4" y="5" w="0" h="19" relatw="1" valign="top" ticker="1" text=":componentname" showlen="1" font="player.font.normal" color="150,180,150" nograb="0" move="1" shadowcolor="150,180,150" shadowx="1" shadowy="-1" fontsize="10" bold="1" align="left" />
 <text x="4" y="5" w="0" h="19" relatw="1" valign="top" ticker="1" text=":componentname" showlen="1" font="player.font.normal" color="255,255,255" nograb="0" move="1" fontsize="10" bold="1" align="left" />
 
 <layer image="close.background" x="-29" y="5" w="25" h="14" relatx="1" />
 <button x="-22" y="7" w="10" h="10" relatx="1" image="titlebar.close" downimage="titlebar.close.down" hoverimage="titlebar.close.down" action="close" tooltip="Close Window" />
</groupdef>

