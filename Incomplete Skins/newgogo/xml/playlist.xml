<container id="playlist" name="Playlist Editor" dynamic="1" component="guid:pl" default_w="275" default_h="300" default_visible="0">
 <layout id="normal" name="Playist Editor" minimum_h="116" minimum_w="275" >
  <custom:winframe fitparent="1" caption="Playlist Editor" nostate="0" nomin="1" dblClickState="shade" />
  <component x="3" y="23" w="-6" h="-46" relatw="1" relath="1" noshowcmdbar="1" autoopen="1" autoclose="0" param="guid:pl" />
  
  <custom:panel x="4" y="-22" w="-8" h="24" relaty="1" relatw="1" />
  
  <custom:button text="Add" x="6" y="-20" h="16" w="30" relaty="1" action="PE_Add" />
  <custom:button text="Rem" x="35" y="-20" h="16" w="30" relaty="1" action="PE_Rem" />
  <custom:button text="Sel" x="64" y="-20" h="16" w="30" relaty="1" action="PE_Sel" />
  <custom:button text="Misc" x="93" y="-20" h="16" w="32" relaty="1" action="PE_Misc" />
  <custom:button text="Manage Playlists" x="-101" y="-20" h="16" w="95" relatx="1" relaty="1" action="PE_List" />
  <rect x="125" y="-19" w="-226" h="14" relaty="1" relatw="1" filled="0" color="custom.panel.border.in" edges="left;right" />
  <rect x="126" y="-18" w="-228" h="12" relaty="1" relatw="1" filled="1" color="custom.panel.background" />
  <layer x="126" y="-17" w="-228" h="5" relaty="1" relatw="1" image="winframe.top.gradient" tile="1" alpha="92" />
  <text x="127" y="-17" w="-230" h="14" relaty="1" relatw="1" display="PE_Info" font="custom.font.hooge553" fontsize="11" color="custom.ticker.text" shadowcolor="custom.ticker.text.shadow" shadowx="1" shadowy="1" valign="top" align="right" />
  <layer x="0" y="0" w="3" h="3" resize="topleft" />
  <layer x="3" y="0" w="-6" h="3" relatw="1" resize="top" />
  <layer x="-3" y="0" w="3" h="3" relatx="1" resize="topright" />
  <layer x="0" y="3" w="3" h="-3" relath="1" resize="left" />
  <layer x="-3" y="3" w="3" h="-6" relatx="1" relath="1" resize="right" />
  <layer x="0" y="-3" w="3" h="3" relaty="1" resize="bottomleft" />
  <layer x="3" y="-3" w="-6" h="3" relatw="1" relaty="1" resize="bottom" />
  <layer x="-3" y="-3" w="3" h="3" relatx="1" relaty="1" resize="bottomright" />
 </layout>
 
 <layout id="shade" name="Playlist Editor Shade"  minimum_h="21" maximum_h="21" minimum_w="275" >
  <custom:winframe fitparent="1" caption="Playlist Editor" nostate="0" nomin="1" dblClickState="normal" notitle="1" />
  <text x="5" y="5" w="-63" h="18" relatw="1" rectrgn="1" showlen="1" display="songname" font="custom.font.hooge553" fontsize="11" color="custom.ticker.text.shadow" shadowcolor="custom.ticker.text" shadowx="0" shadowy="-1" valign="top" />
  <layer id="mousetrap" x="0" y="0" w="-55" h="18" relatw="1" rectrgn="1" move="1" dblClickAction="SWITCH;normal" />
  <layer x="-3" y="3" w="3" h="-6" relatx="1" relath="1" resize="right" />
 </layout>
 
</container>