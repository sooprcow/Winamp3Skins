 <rect id="downrect" x="2" y="2" w="-4" h="-4" relatw="1" relath="1" color="custom.button.down.shadow" edges="left;top;right" alpha="64" />
 <togglebutton x="0" y="1" w="0" h="0" relatw="1" relath="1" id="objbutton" center_image="1" rectrgn="1" />
